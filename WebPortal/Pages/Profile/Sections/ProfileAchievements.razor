<MudStack>
    <MudStack Row AlignItems="AlignItems.Baseline">
        <MudText Typo="Typo.h6">@Localizer["Achievements"]</MudText>
        <MudLink Href="/profile" Typo="Typo.caption">@Localizer["View all"]</MudLink>
    </MudStack>
    <MudGrid>
        @foreach (var achievement in _achievements)
        {
            <MudItem xs="1">
                <MudTooltip Text="Another one bites the dust">
                    <MudIconButton Icon="@achievement" aria-label="@achievement"/>
                </MudTooltip>
            </MudItem>
        }
    </MudGrid>
</MudStack>

@code {

    [Parameter]
    public Microsoft.Graph.User User { get; set; }

    private List<string> _achievements = new()
    {
        Icons.Filled._1k,
        Icons.Filled._10k,
        Icons.Filled.Air,
        Icons.Filled.Alarm,
        Icons.Filled.NearbyError,
        Icons.Filled.Fastfood,
        Icons.Filled.East,
        Icons.Filled.NorthEast,
        Icons.Filled.Quiz,
        Icons.Filled.Fastfood,
        Icons.Filled.Face,
        Icons.Filled.NearbyError,
        Icons.Filled.Image,
        Icons.Filled.Radio,
        Icons.Filled.Javascript,
    };

}