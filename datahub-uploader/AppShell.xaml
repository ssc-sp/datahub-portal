<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="Datahub.Desktop.Uploader.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:Datahub.Desktop.Uploader"
    Shell.FlyoutBehavior="Disabled">

    <ShellContent
        Title="DataHub Uploader"
        ContentTemplate="{DataTemplate local:UploadCodePage}"
        Route="ValidateCodePage" />

    <ShellContent
        Title="DataHub Uploader"
        ContentTemplate="{DataTemplate local:UploadPage}"
        Route="Upload" />

</Shell>
