@using Datahub.Portal.Services.Notification

<p>Good day, </p>
<p>We have a new onboarding request </p>
<p>Please click the following link to view the application: @ApplicationParameters.AppUrl</p>
<p>This email was sent out by @ApplicationParameters.App.ClientContactName for the project @ApplicationParameters.App.ProductName</p>
<p>The project description is: @ApplicationParameters.App.ProjectSummaryDescription</p>

@code {

    [Parameter]
    public OnboardingParameters ApplicationParameters { get; set; }

}